{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "rootDir": "./server",
    "typeRoots": ["node_modules/@types", "./server/types"],
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "exclude": ["node_modules", "dist", "client"]
}
